.gpa-c{
	width: 100%;
	flex-direction: column;
}
.gpa-control{
	width: 100%;
	height: 80rpx;
	background: #66cccc;
	align-items: center;
	justify-content: center;
}
.new-achi{
	color: #fff;
	font-size: 20rpx;
	justify-content: space-between;
	width: 100%;
	height: inherit;
	align-items: center;
	padding: 0 10rpx;
}
.average{
	height: 50rpx;
	width: 300rpx;
	background: #fff;
	color: #66cccc;
	border-radius: 10rpx;
	align-items: center;
	justify-content: center;
}
.import,.add{
	width: 60rpx;
	height: inherit;
	justify-content: center;
	align-items: center;
}
.iconfont{
	font-size: 20rpx;
}
.item-c{
	width: 100%;
	flex-direction: column;
	min-height: 1060rpx;
}
.item{
	width: 100%;
	height: 80rpx;
	border-bottom: 1px solid #eee;
}
.tr{
	color: #66cccc;
}
.a,.b,.c,.d{
	height: inherit;
	align-items: center;
	justify-content: center;
	font-size: 25rpx;
}
.tr input{
	width: inherit;
	height: inherit;
	border-bottom: 1px solid transparent;
	text-align: center;
	box-sizing: border-box;
	transition: border-color 0.3s;
	z-index: 30;
}
.active{
	border-color: #66cccc !important;
}
.a{
	width: 200rpx;
	position: relative;
}
.a1,.a2,.b1,.c1,.d1{
	width: inherit;
	height: inherit;
	align-items: center;
	justify-content: center;
	overflow-x: hidden;
}
.a1{
	position: absolute;
	color: lightgrey;
	opacity: 0.3;
	z-index: 3;
	font-size: 20rpx;
}
.a2{
	z-index: 5;
}
.b{
	width: 200rpx;
}
.c{
	width: 200rpx;
}
.d{
	width: 150rpx;
}
.tips{
	width: 100%;
	padding: 0 30rpx;
	font-size: 20rpx;
	color: lightgrey;
}