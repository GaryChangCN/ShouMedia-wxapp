.address-c{
	width: 100%;
	flex-direction: column;
}
.search-c{
	width: 100%;
	height: 80rpx;
	background: #66cccc;
	align-items: center;
	justify-content: space-between;
}
.search-holder{
	font-size: 25rpx;
	opacity: 0.3;
	color: #2b6071;
}
.search-c>input{
	background-color: #aae7e7;
	width:600rpx;
	border-radius: 10rpx;
	margin-left: 20rpx;
	height:60rpx;
	color: #fff;
	padding-left: 20rpx;
	font-size: 25rpx;
}
.search-c>.button{
	border-color: transparent;
	font-size: 25rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	height: 60rpx;
	color: #fff;
}
.result-c{
	width: 100%;
	flex-direction: column;
}
.result-c>.item{
	width: 100%;
	flex-direction: column;
	border-bottom: 1px solid lightgrey;
	padding: 30rpx 70rpx;
}
.result-c>.item view{
	font-size: 25rpx;
	justify-content: space-between;
	align-items: center;
	height: 50rpx;
}

.result-c .mobile{
	text-decoration: underline;
	color: #66cccc;
}
.result-c>.item text{
	width: 300rpx;
	height: 50rpx;
	display: flex;
	justify-content: flex-start;
	align-items: center;
}